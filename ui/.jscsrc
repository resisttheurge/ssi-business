{
  "preset": "airbnb",
  "esnext" : true,
  "requireSemicolons": false,
  "validateLineBreaks": false,
  "disallowMultipleVarDecl": false,
  "requireTrailingComma": false,
  "requireCommaBeforeLineBreak": false,
  "disallowTrailingWhitespace": false,
  "requireCamelCaseOrUpperCaseIdentifiers": false,
  "safeContextKeyword": false,
  "disallowSpacesInsideParentheses": false,
  "disallowSpacesInsideBrackets": false,
  "disallowSpacesInsideArrayBrackets": false
}
